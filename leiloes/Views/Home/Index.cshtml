@model IEnumerable<leiloes.Models.LeilaoViewModel>

<h2>Leilões Ativos</h2>

<ul>
    @foreach (var leilao in Model)
    {
        <li>
            @leilao.NomeItem - Valor Atual: @leilao.ValorAtualLicitacao
            <!-- Link para a página do leilão (a ser implementado no frontend) -->
        </li>
    }
</ul>
